<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="fuelTypeVM" type="com.abdulsapplication.app.modules.fueltype.data.viewmodel.FuelTypeVM"/>
    </data>
    <LinearLayout android:id="@+id/linearFuelType"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowdown"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_22pxv"
            android:paddingTop="@dimen/_22pxv" style="@style/groupStylewhite_1">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowarrowdown"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowdownOne"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_down"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCancel"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_308pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{fuelTypeVM.fuelTypeModel.txtCancel}"
                        style="@style/txtPoppinsmedium16" tools:text="@string/lbl_cancel"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtWhatisthe"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{fuelTypeVM.fuelTypeModel.txtWhatisthe}"
                style="@style/txtPoppinssemibold22" tools:text="@string/msg_what_is_the_fuel"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowdiesel"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDiesel"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{fuelTypeVM.fuelTypeModel.txtDiesel}"
                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_diesel"/>
                <ImageView android:id="@+id/imageArrowrightOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_321pxh"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <View android:background="@color/blue_gray_100"
                android:id="@+id/lineLinesixOne"
                android:layout_gravity="end"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_405pxh"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowpetrol"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPetrol"
                    android:layout_gravity="center_vertical"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{fuelTypeVM.fuelTypeModel.txtPetrol}"
                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_petrol"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_322pxh"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <View android:background="@color/blue_gray_100"
                android:id="@+id/lineLinesixThree"
                android:layout_gravity="end"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_405pxh"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowpetrolcnglpg"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:id="@+id/txtPetrolcnglpg"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{fuelTypeVM.fuelTypeModel.txtPetrolcnglpg}"
                    style="@style/txtPoppinsmedium14" tools:text="@string/msg_petrol_cng_lpg"/>
                <ImageView android:id="@+id/imageArrowrightFive"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_78pxh"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrow_right"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right"/>
            </LinearLayout>
            <View android:background="@color/blue_gray_100"
                android:id="@+id/lineLinesixFive"
                android:layout_gravity="end"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_405pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
