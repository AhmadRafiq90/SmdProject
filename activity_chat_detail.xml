<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="chatDetailVM" type="com.abdulsapplication.app.modules.chatdetail.data.viewmodel.ChatDetailVM"/>
    </data>
    <FrameLayout android:id="@+id/frameChatDetail"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageImage"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_group_129}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_group_129"/>
        <LinearLayout android:id="@+id/linearColumnarrowdown"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_16pxv" android:paddingTop="@dimen/_16pxv">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowdown"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:paddingEnd="@dimen/_22pxh" android:paddingStart="@dimen/_22pxh">
                    <ImageView android:id="@+id/imageArrowdownOne"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_down"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
                    <ImageView android:id="@+id/imageImageOne"
                        android:layout_height="@dimen/_39pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_width="@dimen/_39pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_mask_group_53x53"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_mask_group_53x53"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtUniqueauto"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{chatDetailVM.chatDetailModel.txtUniqueauto}"
                        style="@style/txtPoppinssemibold18" tools:text="@string/msg_unique_auto_spares"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <androidx.appcompat.widget.SearchView
                android:background="@drawable/rectangle_bg_white_a700_radius_20"
                android:gravity="end" android:id="@+id/searchViewSearch"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent"
                android:theme="@style/searchViewsearch"
                app:iconifiedByDefault="false"
                app:queryBackground="@android:color/transparent" app:queryHint="@string/msg_dfd_waterpump_available"/>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRow"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImageThree"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_97pxh"
                    android:layout_width="@dimen/_130pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_20pxh}"
                    imageRes="@{@drawable/img_image_97x130}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image_97x130"/>
                <ImageView android:id="@+id/imageImageFive"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_97pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_width="@dimen/_130pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_20pxh}"
                    imageRes="@{@drawable/img_image_1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image_1"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowoverflowmenu"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_75pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_13pxh"
                android:paddingStart="@dimen/_13pxh"
                android:paddingTop="@dimen/_6pxv" style="@style/groupStyleorange_50cornerRadius">
                <ImageView android:id="@+id/imageOverflowmenu"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_overflow_menu"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_overflow_menu"/>
                <FrameLayout android:id="@+id/frameStacktime"
                    android:layout_gravity="bottom"
                    android:layout_height="@dimen/_30pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_211pxh">
                    <ImageView android:id="@+id/imageImageSix"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_13pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group_526"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group_526"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.05"
                        android:singleLine="true"
                        android:text="@{chatDetailVM.chatDetailModel.txtTime}"
                        style="@style/txtPoppinsmedium12_3" tools:text="@string/lbl_0_15"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime1"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.05"
                        android:singleLine="true"
                        android:text="@{chatDetailVM.chatDetailModel.txtTime1}"
                        style="@style/txtPoppinsmedium12_3" tools:text="@string/lbl_11_45_am"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageImageSeven"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_44pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_mask_group_53x53"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_mask_group_53x53"/>
            </LinearLayout>
            <androidx.appcompat.widget.SearchView
                android:background="@drawable/rectangle_bg_white_a700_radius_20"
                android:id="@+id/searchViewSearch1"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:theme="@style/searchViewsearch"
                app:iconifiedByDefault="false"
                app:queryBackground="@android:color/transparent" app:queryHint="@string/msg_lorem_ipsum_dummy"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnvoluptate"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_166pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingBottom="@dimen/_8pxv"
                android:paddingEnd="@dimen/_17pxh"
                android:paddingStart="@dimen/_17pxh"
                android:paddingTop="@dimen/_8pxv" style="@style/groupStylewhite_A700cornerRadius">
                <TextView android:id="@+id/txtVoluptatevelit"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.05"
                    android:text="@{chatDetailVM.chatDetailModel.txtVoluptatevelit}"
                    style="@style/txtPoppinsregular12" tools:text="@string/msg_voluptate_velit"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageImageEight"
                android:layout_height="@dimen/_97pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="@dimen/_130pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_mask_group_97x130}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_mask_group_97x130"/>
            <View android:id="@+id/viewView"
                android:layout_height="@dimen/_222pxv" android:layout_width="@dimen/_377pxh"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_radius_10"
                android:drawableEnd="@drawable/img_menu"
                android:drawablePadding="@dimen/_30pxh"
                android:drawableStart="@drawable/drawable_img_thumbsup"
                android:ellipsize="end"
                android:hint="@string/msg_start_typin_here"
                android:id="@+id/etThumbsup" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_21pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_14pxh"
                android:paddingStart="@dimen/_22pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@={chatDetailVM.chatDetailModel.etThumbsupValue}"
                android:textColorHint="@color/blue_gray_700"
                style="@style/etSolidRounded" tools:ignore="Autofill"/>
        </LinearLayout>
    </FrameLayout>
</layout>
