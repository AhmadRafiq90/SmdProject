                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowrequestid1"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_19pxh"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_10pxv" style="@style/groupStyleamber_500cornerRadius">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtRequestidOne"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_7pxh"
                            android:singleLine="true"
                            android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtRequestidOne}"
                            style="@style/txtPoppinssemibold14" tools:text="@string/lbl_request_id"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAb899397"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtAb899397}"
                            style="@style/txtPoppinssemibold14_1" tools:text="@string/lbl_ab899397"/>
                    </LinearLayout>
                    <ImageView android:gravity="center_horizontal"
                        android:id="@+id/imageImageTwo"
                        android:layout_height="@dimen/_46pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
