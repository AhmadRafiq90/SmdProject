                        tools:ignore="ContentDescription" tools:src="@drawable/img_group_501"/>
                    <View android:background="@color/gray_200"
                        android:id="@+id/lineLinenineThree"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_46pxv" android:layout_width="@dimen/_389pxh"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
