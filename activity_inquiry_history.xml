    <data>
        <variable name="inquiryHistoryVM" type="com.abdulsapplication.app.modules.inquiryhistory.data.viewmodel.InquiryHistoryVM"/>
    </data>
    <FrameLayout android:id="@+id/frameInquiryHistory"
