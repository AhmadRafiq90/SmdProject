                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_7pxh"
                            android:singleLine="true"
                            android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtRequestid}"
