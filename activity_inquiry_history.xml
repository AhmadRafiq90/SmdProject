                        android:id="@+id/txtInquiryhistory"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_109pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtInquiryhistory}"
                        style="@style/txtPoppinssemibold18" tools:text="@string/lbl_inquiry_history"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <FrameLayout android:id="@+id/frameStackenquiryon"
                android:layout_gravity="center_horizontal"
