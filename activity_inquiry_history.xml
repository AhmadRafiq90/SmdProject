                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowdateOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDateOne"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_7pxh"
                            android:singleLine="true"
                            android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtDateOne}"
                            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_date"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDate2"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_253pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtDate2}"
                            style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_20_11_2021"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTextnoteOne"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtTextnoteOne}"
                        style="@style/txtPoppinsmedium14" tools:text="@string/lbl_text_note"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDftwaterpump1"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{inquiryHistoryVM.inquiryHistoryModel.txtDftwaterpump1}"
                        style="@style/txtPoppinsmedium14_1" tools:text="@string/msg_dft_water_pump"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnViewDetails1"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="@dimen/_112pxh"
                        android:paddingBottom="@dimen/_3pxv"
                        android:paddingEnd="@dimen/_13pxh"
                        android:paddingStart="@dimen/_13pxh"
                        android:paddingTop="@dimen/_3pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_view_details"
