<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cartVM" type="com.trendysapplication.app.modules.cart.data.viewmodel.CartVM"/>
    </data>
    <FrameLayout android:id="@+id/frameCart"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStackarrowdown"
            android:layout_gravity="center"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageImage"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_group_129}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group_129"/>
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowarrowdown"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_19pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowdownOne"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrow_down"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_down"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCart"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_155pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{cartVM.cartModel.txtCart}"
                        style="@style/txtPoppinssemibold18" tools:text="@string/lbl_cart"/>
                    <FrameLayout android:id="@+id/frameStackshoppingca"
                        android:layout_height="@dimen/_33pxv"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_139pxh" android:layout_width="@dimen/_35pxh">
                        <FrameLayout android:id="@+id/frameStackview"
                            android:layout_gravity="top|end"
                            android:layout_height="@dimen/_17pxv" android:layout_width="@dimen/_16pxh">
                            <View
                                android:background="@drawable/rectangle_bg_white_a700_radius_8"
                                android:id="@+id/viewView"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_16pxh" android:layout_width="@dimen/_16pxh"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtOne"
                                android:layout_gravity="center"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{cartVM.cartModel.txtOne}"
                                style="@style/txtPoppinsmedium11" tools:text="@string/lbl_1"/>
                        </FrameLayout>
                        <ImageView
                            android:id="@+id/imageShoppingcartfiv"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_shopping_cart_5"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_shopping_cart_5"/>
                    </FrameLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </FrameLayout>
        <LinearLayout android:id="@+id/linearColumnviewOne"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_72pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <View android:id="@+id/viewViewOne"
                android:layout_height="1542dp" android:layout_width="@dimen/_428pxh"/>
            <LinearLayout android:id="@+id/linearColumn"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingEnd="@dimen/_8pxh"
                android:paddingStart="@dimen/_8pxh" style="@style/groupStylewhite_A700">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumndftwaterp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_13pxv" style="@style/groupStylewhite_1">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDftwaterpump"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{cartVM.cartModel.txtDftwaterpump}"
                        style="@style/txtPoppinsmedium14_1" tools:text="@string/msg_dft_water_pump_aw41262"/>
                    <View android:background="@color/blue_gray_100"
                        android:id="@+id/lineLinesevenOne"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_376pxh"/>
                    <LinearLayout
                        android:id="@+id/linearRowscreenshot20"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageScreenshot2021"
                            android:layout_height="@dimen/_64pxh"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_68pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_screenshot_2021_11_16_64x68}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_screenshot_2021_11_16_64x68"/>
                        <LinearLayout
                            android:id="@+id/linearColumnprice"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:id="@+id/linearRowprice"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_6pxh"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtPrice"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{cartVM.cartModel.txtPrice}"
                                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_price"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtPrice1"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_167pxh"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{cartVM.cartModel.txtPrice1}"
                                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_pkr_57000"/>
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:id="@+id/linearRowquantity"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtQuantity"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_3pxv"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{cartVM.cartModel.txtQuantity}"
                                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_quantity"/>
                                <LinearLayout
                                    android:gravity="center_vertical"
                                    android:id="@+id/linearRoweditminus"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_121pxh"
                                    android:layout_width="wrap_content" android:orientation="horizontal">
                                    <ImageButton
                                    android:id="@+id/btnEditminusOne"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_28pxh"
                                    android:layout_width="@dimen/_28pxh"
                                    android:paddingHorizontal="@dimen/_2pxh"
                                    android:paddingVertical="@dimen/_2pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_edit_minus_circle" tools:ignore="ContentDescription"/>
                                    <TextView android:ellipsize="end"
                                    android:id="@+id/txtOneOne"
                                    android:layout_gravity="top"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_2pxv"
                                    android:layout_marginStart="@dimen/_17pxh"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{cartVM.cartModel.txtOneOne}"
                                    style="@style/txtPoppinsmedium16" tools:text="@string/lbl_1"/>
                                    <ImageButton
                                    android:id="@+id/btnEditplusOne"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="@dimen/_28pxh"
                                    android:layout_marginStart="@dimen/_20pxh"
                                    android:layout_width="@dimen/_28pxh"
                                    android:paddingHorizontal="@dimen/_2pxh"
                                    android:paddingVertical="@dimen/_2pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_edit_plus_circle" tools:ignore="ContentDescription"/>
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:id="@+id/linearRowsubtotal"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_6pxh"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtSubtotal"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{cartVM.cartModel.txtSubtotal}"
                                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_sub_total"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtPrice2"
                                    android:layout_gravity="center_vertical"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_135pxh"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{cartVM.cartModel.txtPrice2}"
                                    style="@style/txtPoppinsmedium14" tools:text="@string/lbl_pkr_57000"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowrremove"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnView"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_28pxv"
                            android:layout_width="@dimen/_71pxh"
                            android:paddingBottom="@dimen/_3pxv"
                            android:paddingEnd="@dimen/_18pxh"
                            android:paddingStart="@dimen/_18pxh"
                            android:paddingTop="@dimen/_3pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_view"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded_1" tools:text="@string/lbl_view"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtRremove"
                            android:layout_gravity="center_vertical"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_21pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{cartVM.cartModel.txtRremove}"
                            style="@style/txtPoppinsmedium14_2" tools:text="@string/lbl_rremove"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowsubtotalOne"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:paddingBottom="@dimen/_24pxv"
                    android:paddingEnd="@dimen/_19pxh"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_24pxv" style="@style/groupStylewhite_1">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnsubtotal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSubtotalOne"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{cartVM.cartModel.txtSubtotalOne}"
                            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_sub_total"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtDiscount"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{cartVM.cartModel.txtDiscount}"
                            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_discount"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTotal"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{cartVM.cartModel.txtTotal}"
                            style="@style/txtPoppinssemibold14" tools:text="@string/lbl_total"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumn0zero"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_width="wrap_content" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice3"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{cartVM.cartModel.txtPrice3}"
                            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_pkr_57000"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt0zero"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{cartVM.cartModel.txt0zero}"
                            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_0_00"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice4"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{cartVM.cartModel.txtPrice4}"
                            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_pkr_57000"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnBuyNow"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@string/lbl_buy_now"
                android:textAllCaps="false" style="@style/btnSolid" tools:text="@string/lbl_buy_now"/>
        </LinearLayout>
    </FrameLayout>
</layout>
